<h3>Name: <%= @restaurant.name %></h3>
<h4>Address: <%= @restaurant.address %></h4>
<h4>Phone Number: <%= @restaurant.phone_number %></h4>
<h4>Category: <%= @restaurant.category %></h4>
<ul>
  <% @restaurant.reviews.each do |review| %>

  <li> Review: <%= review.content %> Rating: <%= review.rating %> </li>
  <% end %>

</ul>
<p><%= link_to "Review Restaurant",  new_restaurant_review_path(@restaurant) %> </p>
<%= link_to "Back to restaurants", restaurants_path(@restaurant) %>
